{
    "name": "corcronje/laravel-smsportal",
    "description": "Send text messages via SmsPortal",
    "type": "library",
    "require": {
        "corcronje/smsportal": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CorCronje\\LaravelSmsPortal\\SmsPortalServiceProvider"
            ],
            "aliases": {
                "Client": "CorCronje\\LaravelSmsPortal\\Facades\\Client",
                "Message": "CorCronje\\LaravelSmsPortal\\Facades\\Message"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "CorCronje\\LaravelSmsPortal\\" : "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cor Cronje",
            "email": "cronje.cor@gmail.com"
        }
    ]
}